monitor testEvent {
	constant string eventChannel := "channel1";
	
	action onload() {
		monitor.subscribe(eventChannel);
		
		on all p1() as event1 {
			sendAction(event1.airport, "found!");
		}
		
	}
	
					
			
	action sendAction(integer airport, string str) {
		send e1(airport, str) to eventChannel;
	}
}

