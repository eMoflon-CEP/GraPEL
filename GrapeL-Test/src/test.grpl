import "platform:/resource/Flights/model/Flights.ecore"
import "http://www.eclipse.org/emf/2002/Ecore"

event e1 {
	Airport airport;
	EString string;
}

pattern p1(string:EString) {
  airport : Airport
}

event_pattern testEvent {
	node : e1
	node2 : p1
	node.airport == node2.airport
	node -> node2 {}
}